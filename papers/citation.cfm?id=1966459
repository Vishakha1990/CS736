    function selectPerson(index) {
        var img, i, url;
        if (selected_person >= 0) {
            img = document.getElementById("person_" + selected_person);
            if (img) {
                img.style.opacity = "0.4"; 
                img.style.filter = "alpha(opacity=40)";
            }
        }
        img = document.getElementById("person_" + index);
        if (img) {
            selected_person = index;
            img.style.opacity = "1.0"; 
            img.style.filter = "alpha(opacity=100)";
            flyover = document.getElementById('related_item_flyover');
            flyoverContents = flyoverContentsFor(img);
            innerHTML = '<span class="person-name"><a href="author_page.cfm?id=' + id + '" onclick="feature(5)" target="_new">' + flyoverContents[0] + '</a></span><br/>';
            innerHTML += '<table class="person-concepts">';
            for (i = 0; i < flyoverContents[2].length; i++) {
                if (!i % 2) {
                    innerHTML += '<tr>';
                }
                url = flyoverContents[3][i];
                title = flyoverContents[4][i];
                if (url) {
                    innerHTML += '<td width="250px"><span class="person-concept" onmouseenter="personConceptEnter(' + index + ',' + i + ')"><a href="' + url +  '"' + ' onclick="feature(4)" '  + ' title="' + 'Appears in paper: ' + flyoverContents[4][i] + '" ' + 'target="_new">'+ flyoverContents[2][i] + '</a></span></td>';
                }
                else {
                    innerHTML += '<td width="250px"><span class="person-concept" onmouseenter="personConceptEnter(' + index + ',' + i + ')">' + flyoverContents[2][i] + '</span></td>';
                }
                if (i % 2) {
                    innerHTML += '</tr>';
                }
            }
            if (i % 2) {
                innerHTML += '<td></td></tr>';
            }
            innerHTML += '</table>';
            innerHTML += '<br/>';
            flyover.innerHTML = innerHTML;
            flyover.style.opacity = 8.0;
            flyover.style.filter = "alpha(opacity=80)";  // for ie
        }
    }

    function relatedItemClicked(img) {
        var id = related_authors[img.id.split('_')[1]].id;
		feature(6);
        window.open('author_page.cfm?id=' + id);
    }
	
	function feature(fid) {
		document.cookie = "WATSON=1966459:" + fid;	
	}

    function flyoverContentsFor(img) {
        var index, i, name, tags = [], urls = []; titles = [];
        index = img.id.split('_')[1];
        name = related_authors[index].name;
        id = related_authors[index].id;
        for (i = 0; i < related_authors[index].concepts.length; i++) {
            tags[i] = related_authors[index].concepts[i].label;
            urls[i] = related_authors[index].concepts[i].url;
			titles[i] = related_authors[index].concepts[i].source;
        }
        return ([name, id, tags, urls, titles]);
    }

    function relatedItemEnter(img) {
        document.getElementById('person_concept_explanation').innerHTML = '';
        selectPerson(img.id.split('_')[1]);
		personConceptEnter(img.id.split('_')[1],0); //wg
    }
	
    function personConceptEnter(personIndex, conceptIndex) {
        var source, passage, div, innerHTML;
        source = related_authors[personIndex].concepts[conceptIndex].source;
        passage = related_authors[personIndex].concepts[conceptIndex].passage;
        url = related_authors[personIndex].concepts[conceptIndex].url;
        div = document.getElementById('person_concept_explanation');
        if (source && passage) {
            innerHTML = '<div class="person-concept-title">' + 'In ' + 
			
			
			'<a href="' + url +  '"' + '  onclick="feature(8)" title="' + 'Appears in paper: ' + source + '" ' + 'target="_new">' +
			
			source + '</a></div>' + '<span class="person-concept-author">' + related_authors[personIndex].name + ' wrote:<br/></span><span class="person-concept-passage">&quot;' + passage + '&quot;</span>';
            
			
			div.innerHTML = innerHTML;
        }
        else {
            div.innerHTML = '';
        }
    }

    function toggleAuthors() {
        var div = document.getElementById('related_authors'); 
        if (div.style.display == "none") {
            div.style.display = "block";
			feature(2);
			httpGetAsync("watson_track.cfm");
        }
        else {
            div.style.display = "none";
        }
        var div = document.getElementById('related_item_flyover'); 
        if (div.style.display == "none") {
            div.style.display = "block";
        }
        else {
            div.style.display = "none";
        }
        div = document.getElementById('person_concept_explanation'); 
        if (div.style.display == "none") {
            div.style.display = "block";
            document.getElementById("expandcollapseauthors").src = "images/collapse.png";
        }
        else {
            div.style.display = "none";
            document.getElementById("expandcollapseauthors").src = "images/expand.png";
        }
    }

    function toggleConcepts() {
        var div = document.getElementById('related_concepts'); 
        if (div.style.display == "none") {
            div.style.display = "block";
            document.getElementById("expandcollapseconcepts").src = "images/collapse.png";
			feature(3);
			httpGetAsync("watson_track.cfm");
        }
        else {
            div.style.display = "none";
            document.getElementById("expandcollapseconcepts").src = "images/expand.png";
        }
    }
	
	function httpGetAsync(theUrl) {
		var xmlHttp = new XMLHttpRequest();
		xmlHttp.onreadystatechange = function() {
			/*if (xmlHttp.readyState == 4 && xmlHttp.status == 200)
				callback(xmlHttp.responseText);*/
		}
		xmlHttp.open("GET", theUrl, true); // true for asynchronous 
		xmlHttp.send();
	}
		
</script>



            

			
            <div id="watson" class="watson">
                <h1 class="mediumb-text" style="margin-top:0px; margin-bottom:0px;">
                    <a href="javascript: void(0);"  onclick="toggleAuthors('relatedauthors');" ><img src="images/ProfileSilhouetteBlue.png" alt="" width="30px" height="30px"/></a>
                    &nbsp;&nbsp;
                    <span class="header-text">
                        <a href="javascript: void(0);"  onclick="toggleAuthors('relatedauthors');" >Recent authors with related interests</a>
                        <a href="javascript: void(0);"  onclick="toggleAuthors('relatedauthors');" ><img id="expandcollapseauthors" class="expand-collapse" src="images/expand.png" width="40px" alt="Expand Related Authors" style="vertical-align:bottom;" /></a>
                        &nbsp;&nbsp;
                        <a href="javascript: void(0);"  onclick="toggleConcepts();" >Concepts in this article</a>
                        <a href="javascript: void(0);"  onclick="toggleConcepts();" ><img id="expandcollapseconcepts" class="expand-collapse" src="images/expand.png" width="40px" alt="Expand Paper Concepts" style="vertical-align:bottom;" /></a>
                    </span>
                </h1>
